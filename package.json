    "deploy": "npm-run-all clean build && mmpilot deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunruch/kunruchcreations.com.git"
  },
  "author": "Kanishk Kunal",
  "bugs": {
    "url": "https://github.com/kunruch/kunruchcreations.com/issues"
  },
  "homepage": "https://kunruchcreations.com",
  "devDependencies": {
    "mmpilot": "^0.6.6",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "mmcss": "^0.3.3"
  }
}
