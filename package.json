{
  "name": "kunruchcreations.com",
  "version": "1.0.0",
  "description": "Main website of KunRuch Creations",
  "scripts": {
    "clean": "mmpilot clean",
    "build": "mmpilot build",
    "serve": "mmpilot serve",
    "dev-build": "mmpilot build -d",
    "dev-watch": "mmpilot watch -d",
    "dev": "npm-run-all clean dev-build --parallel dev-watch serve",
